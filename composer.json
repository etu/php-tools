{
    "name": "etu/ET",
    "description": "Some tools for personal projects",
    "keywords": ["Database", "Config"],
    "license": "ISC",

    "authors": [
        {
            "name": "Elis Axelsson",
            "homepage": "http://elis.nu/"
        }
    ],

    "require": {
        "php": ">=5.5.0"
    },

    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "jakub-onderka/php-parallel-lint": "@stable"
    },

    "autoload": {
        "psr-4": {
            "Etu\\PhpTools\\": "src/Etu/PhpTools"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\Etu\\PhpTools\\": "unit/src/Etu/PhpTools"
        }
    }
}
