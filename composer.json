{
    "require-dev": {
        "phpunit/phpunit": "4.2",
        "tnt/phockito": "1.2.0",
        "squizlabs/php_codesniffer": "1.5.2",
        "jakub-onderka/php-parallel-lint": "0.*"
    },

    "repositories": [
        {
            "url": "https://github.com/203webgroup/phockito.git",
            "type": "git"
        }
    ],

    "autoload": {
        "psr-4": {
            "ET\\": "src/ET"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\ET\\": "tests/unit/src/ET"
        }
    }
}
